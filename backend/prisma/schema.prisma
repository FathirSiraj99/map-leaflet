// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model type_place {
  id       String @id @default(uuid())
  typeName String

  place place[]
}

model place {
  id          String @id @default(uuid())
  name        String
  description String

  latitude     String
  longitude    String
  type_place   type_place? @relation(fields: [type_placeId], references: [id])
  type_placeId String?
}
